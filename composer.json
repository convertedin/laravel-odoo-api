{
  "name": "obuchmann/laravel-odoo-api",
  "description": "Laravel connector for Odoo",
  "keywords": [
    "odoo",
    "laravel",
    "package"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Oliver Buchmann",
      "email": "obuchmanncode@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Obuchmann\\LaravelOdooApi\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "~7|~8",
    "darkaonline/ripcord": "^0.1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Obuchmann\\LaravelOdooApi\\Providers\\OdooServiceProvider"
      ],
      "aliases": {
        "Odoo": "Obuchmann\\LaravelOdooApi\\Facades\\Odoo"
      }
    }
  }
}
